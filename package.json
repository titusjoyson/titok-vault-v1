{
  "name": "titok-vault",
  "version": "0.1.0",
  "author": {
    "name": "Titus Joyson",
    "email": "tj.joyson@gmail.com",
    "url": "https://titusjoyson.github.io/"
  },
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.1.4",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0",
    "electron-serve": "^1.0.0",
    "googleapis": "^49.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "react-start": "cross-env BROWSER=none react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-eject": "react-scripts eject",
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "electron-check": "electron-builder install-app-deps",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "react-build && electron-build"
  },
  "build": {
    "appId": "org.titusjoyson.titokvault",
    "productName": "Titok Vault",
    "copyright": "Copyright Â© 2020 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {},
    "linux": {
      "icon": "./build/icons",
      "category": "office",
      "target": [
        "deb"
      ],
      "desktop": {
        "Encoding": "UTF-8"
      }
    },
    "mac": {}
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^22.4.1",
    "nodemon": "^2.0.2"
  }
}
